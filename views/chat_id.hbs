{{>nav}}
<input type="hidden" id="userID" value={{userID}}>
<input type="hidden" id="RoomID" value={{RoomID}}>
<div class="chats_user">
    <ul id="messages" class="ul">
        {{#each massage}}
        <li class="li"><img src="https://img.icons8.com/plasticine/2x/user.png" alt="" width="50px"
                height="50px">{{this.message}}
        </li>
        {{/each}}
    </ul>
    <form action="">
        <textarea id="input_submit"> </textarea>
        <button><img class='send_icon' src="../images/send_icon.png" alt=""></button>
    </form>
</div>

