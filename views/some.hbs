<div class="about_user">
    <div class="user_img">
        <img src={{this.photo}} alt="user">
    </div>
    <div class="user_info">
        <div>{{this.name}}</div>
        <div>{{this.lastname}}</div>
        <div>{{this.email}}</div>
        <div>{{this.group}}</div>
    </div>
    <div>
        <form action="/users/logout" method="POST">
            <input type="submit"value="Log Out">
        </form>
    </div>
    
</div>

